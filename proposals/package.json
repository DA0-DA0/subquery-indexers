{
  "name": "dao-dao-proposals-subquery-indexer",
  "version": "0.0.1",
  "description": "Indexes DAO DAO proposal statuses and votes. Used to get timestamps for proposal states as well as provide data for the inbox.",
  "main": "dist/index.js",
  "scripts": {
    "build": "subql build",
    "prepack": "rm -rf dist && npm build",
    "test": "jest",
    "codegen": "subql codegen && replace-in-files --string '@subql/types' --replacement '@subql/types-cosmos' src/types/models/*",
    "start:docker": "docker-compose pull && docker-compose up --remove-orphans",
    "publish": "subql publish",
    "deploy:production": "subql deployment:deploy -d --org=NoahSaso --projectName=dao-dao-proposals"
  },
  "homepage": "https://github.com/DA0-DA0/subquery-indexers/tree/main/proposals",
  "repository": "https://github.com/DA0-DA0/subquery-indexers",
  "files": [
    "dist",
    "schema.graphql",
    "project.yaml"
  ],
  "author": "Noah Saso <noahsaso@gmail.com>",
  "license": "Clear BSD",
  "devDependencies": {
    "@cosmjs/stargate": "^0.28.9",
    "@subql/cli": "^1.1.0",
    "replace-in-files-cli": "^2.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@subql/types-cosmos": "^0.1.0",
    "@types/node": "^17.0.21",
    "pino": "^7.8.0",
    "ts-proto": "^1.112.1",
    "tslib": "^2.3.1"
  }
}
